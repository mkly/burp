{
  "name": "buuur",
  "version": "0.1.0",
  "homepage": "https://github.com/mkly/buuur",
  "authors": [
    "Mike Lay <mike@mkly.io>"
  ],
  "main": "index.html",
  "moduleType": [
    "amd"
  ],
  "license": "AGPLv3",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "gif.js": "~0.1.6",
    "socketio-client": "~1.2.1",
    "underscore": "~1.8.2",
    "requirejs": "~2.1.16",
    "hammer.js": "~2.0.4"
  },
  "install": {
    "path": {
      "js": "app/public/js",
      "css": "app/public/css"
    },
    "sources": {
      "gif.js": [
        "bower_components/gif.js/dist/gif.js",
        "bower_components/gif.js/dist/gif.worker.js"
      ],
      "underscore": [
        "bower_components/underscore/underscore-min.js"
      ],
      "socketio-client": [
        "bower_components/socketio-client/socket.io.js"
      ],
      "requirejs": [
        "bower_components/requirejs/require.js"
      ],
      "hammer.js": [
        "bower_components/hammer.js/hammer.js"
      ],
      "pure": [
        "bower_components/pure/pure-min.css"
      ]
    }
  }
}
